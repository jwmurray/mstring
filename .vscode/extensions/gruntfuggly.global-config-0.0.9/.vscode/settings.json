        "clocale": "cpp",
        "cwctype": "cpp",
        "cstdarg": "cpp",
        "cstdio": "cpp",
        "cstdlib": "cpp",
        "cstddef": "cpp",
        "ctime": "cpp",
        "QTimer": "cpp",
        "*harray": "cpp",
        "system_error": "cpp",
        "QNetworkReply": "cpp",
        "istream": "cpp",
        "functional": "cpp",
        "*h*h*htuple": "cpp",
        "tuple": "cpp",
        "utility": "cpp",
        "array": "cpp",
        "QString": "cpp",
        "*.tcc": "cpp",
        "cstdint": "cpp",
        "exception": "cpp",
        "new": "cpp",
        "limits": "cpp",
        "*.i": "cpp",
        "vector": "cpp",
        "bitset": "cpp",
        "qdomdocument": "cpp",
        "cmath": "cpp",
        "cstring": "cpp",
        "fstream": "cpp",
        "iomanip": "cpp",
        "iosfwd": "cpp",
        "iostream": "cpp",
        "memory": "cpp",
        "ostream": "cpp",
        "sstream": "cpp",
        "stdexcept": "cpp",
        "streambuf": "cpp",
        "qdebug": "cpp",
        "algorithm": "cpp",
        "qframe": "cpp",
        "qdatetime": "cpp",
        "complex": "cpp",
        "hash_map": "cpp",
        "hash_set": "cpp",
        "deque": "cpp",
        "forward_list": "cpp",
        "list": "cpp",
        "unordered_map": "cpp",
        "unordered_set": "cpp",
        "quuid": "cpp",
        "numeric": "cpp",
        "cinttypes": "cpp",
        "qlist": "cpp",
        "qdomelement": "cpp",
        "qtextstream": "cpp",
        "qtemporaryfile": "cpp",
        "qvariantmap": "cpp",
